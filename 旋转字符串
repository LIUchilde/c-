 //如果A=‘youzan’，B=‘zanyou’，A按‘you’‘zan’切割换位后得到‘zanyou’和B相同返回true。
 
 
 bool solve(string A, string B) {
        // write code here
        for(int i=0;i<A.size()-1;i++)
        {
            if(A.substr(i,A.size()-i)+A.substr(0,i)==B){
                return true;
            }
      
        }
        return false;
    }
