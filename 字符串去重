#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
void fun(string s1,string s2){
	if (s1.size() == 0){
		return;
	}
	int size = s1.size();
	for (int i = 0; i < size; i++){
		if (s1.find(s1[i]) == i){
			s2 += s1[i];
		}
	}
	for (int i = 0; i != s2.size(); i++){
		cout << s2[i];
	}
}
int main(){
	string s1, s2;
	getline(cin, s1);
	fun(s1,s2);
	return 0;
}
